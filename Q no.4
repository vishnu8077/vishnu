#include <stdio.h>
int main(void) {
 int days, years, weeks, remainingDays;
 printf("Enter the number of days: ");
 scanf("%d", &days);
 years = days / 365;
 weeks = (days % 365) / 7;
 remainingDays = (days % 365) % 7;
 printf("Years: %d\n", years);
 printf("Weeks: %d\n", weeks);
 printf("Days: %d\n", remainingDays);
 return 0;
}
